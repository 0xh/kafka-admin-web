div

  div(v-if='part === "replica-alert"')
    h3 Replica Manager(Alert)
    table.table
      thead: tr
        th Broker
        th UnderReplicatedPartitions
        th OfflinePartitionsCount
        th ActiveControllerCount
        th PreferredReplicaImbalanceCount
      tbody: tr(v-for='broker in brokers')
        td {{ broker.broker }}
        td {{ broker.UnderReplicatedPartitions | value | number }}
        td {{ broker.OfflinePartitionsCount | value | number }}
        td {{ broker.ActiveControllerCount | value | number }}
        td {{ broker.PreferredReplicaImbalanceCount | value | number }}


  div(v-if='part === "replica-count"')
    h3 Replica Manager(Count Metrics)
    table.table
      thead: tr
        th Broker
        th LeaderCount
        th PartitionCount
        th MaxLag
      tbody: tr(v-for='broker in brokers')
        td {{ broker.broker }}
        td {{ broker.LeaderCount | value | number }}
        td {{ broker.PartitionCount | value | number }}
        td {{ broker.MaxLag | value | number }}


  div(v-if='part === "replica-rate"')
    h3 Replica Manager(Rate Metrics)
    table.table
      thead: tr
        th Broker
        th LeaderElectionRateAndTimeMs
        th(colspan=3) IsrExpandsPerSec(1m/5m/15m)
        th(colspan=3) IsrShrinksPerSec(1m/5m/15m)
      tbody: tr(v-for='broker in brokers')
        td {{ broker.broker }}
        td {{ broker.LeaderElectionRateAndTimeMs | value | number }}
        td {{ broker.IsrExpandsPerSec | value('OneMinuteRate') | value | number }}
        td {{ broker.IsrExpandsPerSec | value('FiveMinuteRate') | value | number }}
        td {{ broker.IsrExpandsPerSec | value('FifteenMinuteRate') | value | number }}
        td {{ broker.IsrShrinksPerSec | value('OneMinuteRate') | value | number }}
        td {{ broker.IsrShrinksPerSec | value('FiveMinuteRate') | value | number }}
        td {{ broker.IsrShrinksPerSec | value('FifteenMinuteRate') | value | number }}


  div(v-if='part === "io"')
    h3 Messages/Bytes IO
    table.table
      thead: tr
        th Broker
        th(colspan=3) MessagesInPerSec(1m/5m/15m)
        th(colspan=3) BytesInPerSec(1m/5m/15m)
        th(colspan=3) BytesOutPerSec(1m/5m/15m)
        th(colspan=3) BytesRejectedPerSec(1m/5m/15m)
      tbody: tr(v-for='broker in brokers')
        td {{ broker.broker | number }}
        td(v-on:click='toggleChart(broker.broker, "MessagesInPerSec", "OneMinuteRate")') {{ broker.MessagesInPerSec | value('OneMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "MessagesInPerSec", "FiveMinuteRate")') {{ broker.MessagesInPerSec | value('FiveMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "MessagesInPerSec", "FifteenMinuteRate")') {{ broker.MessagesInPerSec | value('FifteenMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "BytesInPerSec", "OneMinuteRate")') {{ broker.BytesInPerSec | value('OneMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "BytesInPerSec", "FiveMinuteRate")') {{ broker.BytesInPerSec | value('FiveMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "BytesInPerSec", "FifteenMinuteRate")') {{ broker.BytesInPerSec | value('FifteenMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "BytesOutPerSec", "OneMinuteRate")') {{ broker.BytesOutPerSec | value('OneMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "BytesOutPerSec", "FiveMinuteRate")') {{ broker.BytesOutPerSec | value('FiveMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "BytesOutPerSec", "FifteenMinuteRate")') {{ broker.BytesOutPerSec | value('FifteenMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "BytesRejected", "OneMinuteRate")') {{ broker.BytesRejected | value('OneMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "BytesRejected", "FiveMinuteRate")') {{ broker.BytesRejected | value('FiveMinuteRate') | value | number }}
        td(v-on:click='toggleChart(broker.broker, "BytesRejected", "FifteenMinuteRate")') {{ broker.BytesRejected | value('FifteenMinuteRate') | value | number }}


  div(v-if='part === "log"')
    h3 Log / Disk
    table.table
      thead: tr
        th Broker
        th LogEndOffset
        th LogCount
        th LogSize
        th LogNumLogSegments
      tbody: tr(v-for='broker in brokers')
        td {{ broker.broker }}
        td {{ broker.LogEndOffset | value | number }}
        td {{ broker.LogEndOffset && broker.LogStartOffset && (broker.LogEndOffset.Value - broker.LogStartOffset.Value) | number }}
        td {{ broker.Size | value | number }}
        td {{ broker.NumLogSegments | value | number }}


  div(v-if='part === "request"')
    h3 Network
    table.table
      thead: tr
        th Broker
        th(colspan=3) RequestPerSec(1m/5m/15m)
      tbody: tr(v-for='broker in brokers')
        td {{ broker.broker | number }}
        td {{ broker.totalRequestsPerSec | value('OneMinuteRate') | value | number }}
        td {{ broker.totalRequestsPerSec | value('FiveMinuteRate') | value | number }}
        td {{ broker.totalRequestsPerSec | value('FifteenMinuteRate') | value | number }}


  div(v-if='part === "deplay"')
    h3 DeplayedOperationPurgatory
    table.table
      thead: tr
        th Broker
        th(colspan=3) Produce Delay(1m/5m/15m)
        th(colspan=3) Fetch Delay(1m/5m/15m)
      tbody: tr(v-for='broker in brokers')
        td {{ broker.broker | number }}

    //- template(v-for='broker in brokers')
    //-   pre {{ broker }}